{
    "version": 2,
    "builds": [
        {
            "src": "backend/othello",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/src/public",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/othello/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/src/public"
        }
    ]
}
